<theme>
    <formatVersion>3</formatVersion>

	

	<view name="system">


			
	</view>
	
	<feature supported="carousel">

		<view name="system">

		
		</view>

	</feature>

	<view name="basic, detailed, video">
	

		
		<text name="md_rating, md_developer, md_publisher, md_genre, md_players, md_playcount, md_releasedate">
			<color>d7e894</color>
		</text>
		
		<datetime name="md_releasedate">
			<color>aec440</color>
		</datetime>
		
		<text name="md_description">
		<color>204631</color>
		</text>
	
		<rating name="md_rating">
			<filledPath>./../art/heart_filled_gb.png</filledPath>
			<unfilledPath>./../art/heart_empty_gb.png</unfilledPath>
		</rating>
		
		<textlist name="gamelist">
			<selectorColor>d7e894</selectorColor>
			<selectedColor>204631</selectedColor>
			<primaryColor>527f39</primaryColor>
			<secondaryColor>d7e894</secondaryColor>
		</textlist>
		

		

	
	
		<image name="left_window_border, right_window_border" extra="true">
			<color>aec440</color>
		</image>
		
		<image name="left_window_body, right_window_body" extra="true">
			<color>d7e894</color>
		</image>
		
		
		<image name="right_window_box, left_window_box" extra="true">
			<color>aec440</color>
		</image>

		
		<image name="bar_1, bar_2, bar_3, bar_4 bar_5 bar_6 bar_7 bar_8" extra="true">
			<color>204631</color>
		</image>		
		
		<text name="players_label" extra="true">
			<color>d7e894</color>
		</text>

		
		<image name="player_box" extra="true">
			<color>527f39</color>		
		</image>

		<text name="publisher_label, developer_label, genre_label, rating_label, played_label" extra="true">
			<color>aec440</color>
		</text>

		<datetime name="md_releasedate">
			<color>aec440</color>
		</datetime>	

		
		
	

	</view>
	
</theme>